<?xml version="1.0"?>
<Container version="2">
  <Name>CouchDb</Name>
  <Repository>apache/couchdb</Repository>
  <Registry>https://github.com/apache/couchdb-docker</Registry>
  <Network>bridge</Network>
  <MyIP/>
  <Shell>sh</Shell>
  <Privileged>false</Privileged>
  <Support>https://forums.unraid.net/index.php?topic=38486.0</Support>
  <Project>https://github.com/apache/couchdb-docker</Project>
  <Overview>CouchDb Latest&#xD;
Document-oriented NoSQL database.</Overview>
  <Category>Other:</Category>
  <WebUI>http://[IP]:[PORT:5984]/_utils</WebUI>
  <TemplateURL>https://raw.githubusercontent.com/spants/unraidtemplates/master/Spants/CouchDB-v2.x.xml</TemplateURL>
  <Icon>https://couchdb.apache.org/image/couch@2x.png</Icon>
  <ExtraParams/>
  <PostArgs/>
  <CPUset/>
  <DateInstalled>1725399103</DateInstalled>
  <DonateText/>
  <DonateLink/>
  <Requires/>
  <Config Name="WebUI" Target="5984" Default="" Mode="tcp" Description="" Type="Port" Display="always" Required="true" Mask="false">5984</Config>
  <Config Name="Appdata" Target="/opt/couchdb/data/" Default="/mnt/user/appdata/couchdb/" Mode="rw" Description="" Type="Path" Display="always" Required="true" Mask="false">/mnt/user/appdata/couchdb/</Config>
  <Config Name="Config" Target="/opt/couchdb/etc/local.d/" Default="/mnt/user/appdata/couchdb/config/" Mode="rw" Description="" Type="Path" Display="always" Required="true" Mask="false">/mnt/user/appdata/couchdb/config/</Config>
  <Config Name="Database User" Target="COUCHDB_USER" Default="admin" Mode="" Description="Default: admin" Type="Variable" Display="always" Required="true" Mask="false">admin</Config>
  <Config Name="Database Password" Target="COUCHDB_PASSWORD" Default="" Mode="" Description="Set your password" Type="Variable" Display="always" Required="true" Mask="false">SET-YOUR-PASSWORD</Config>
</Container>
